{
  "hosting": {
    "public": "alliyn-business-matchmaker/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/auth/signin",
        "type": 302
      }
    ]
  }
}
